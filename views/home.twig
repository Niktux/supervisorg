{% extends 'admin/common/body.twig' %}

{% block title %}Supervisorg{% endblock %}
{% block skin %}skin-blue{% endblock %}

{% block header %}
    {% include 'header.twig' %}
{% endblock %}

{% block sidebar %}
    {{ render(url('sidebar')) }}
{% endblock %}

{% block pageTitle %}
    {{title}}
    {% if subTitle is defined %}
        <small>{{ subTitle }}</small>
    {% endif %}
{% endblock %}

{% block body %}
    {% for process in processes %}
        {% embed 'partials/process_tile.twig' %}{% endembed %}
    {% endfor %}
{% endblock %}


{% block specificsJavascripts %}
{% javascripts
    '../assets/js/libs/mousetrap.min.js'
    '../assets/js/libs/jquery.countdown360.min.js'
    '../assets/js/timer.js'
    '../assets/js/search.js'
%}
<script type="text/javascript" src="/{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock specificsJavascripts %}
